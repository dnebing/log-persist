<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.logging.persist.model.LoggingConfig" />
	<class name="com.liferay.logging.persist.model.impl.LoggingConfigImpl" table="LOGCFG">
		<id name="logConfigId" type="long">
			<generator class="assigned" />
		</id>
		<property name="logger" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="level" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>